<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <title>title</title>
    <link rel="stylesheet" type="text/css" href="../css/api.css"/>
    <style>
        *{
			padding: 0rem;
			margin: 0rem;
			list-style: none;
			text-decoration: none;
		}

		html{
			font-size: 20px;
		}

		.header {
			width: 100%;
			height: 48px;
			border-bottom: 1px solid #ccc;
			position: relative;
		}

		.header img {
			width: 22px;
			height: 22px;
			position: absolute;
			left: 15px;
			top: 13px;
		}

		.header p {
			width: 100%;
			height: 48px;
			line-height: 48px;
			color: #ff6633;
			font-family: 微软雅黑;
			font-size: 16px;  
			text-align: center;
		}

		.header .guanli {
			width: 25px;
			height: 49px;
			line-height: 49px;
			position: absolute;
			right:15px;
			color: #ff6633;
			top: 0px;
			font-size: 12px;
		}
    </style>
</head>
<body>
    <div class="wrap">
        <div class="header" id="header">
          <img src="../icon/back_orange.png" id="back">
          <p>早餐</p>
        </div>
        <div class="content">
        </div>
    </div>
</body>
<script type="text/javascript" src="../script/api.js"></script>
<script type="text/javascript">
    apiready = function() {
        headerH = $api.offset(header).h;
        $api.fixStatusBar(headerH);
        var main_height = api.winHeight;
        api.openFrame({
            name: 'Shiyun_zaocan_frame',
            url: '../frame/Shiyun_zaocan_frame.html',
            rect: {
                x: 0,
                y: headerH,
                w: 'auto',
                h: main_height
            },
            animation:{
                type:'none',
                duration:300               
            },
            pageParam: {
                name: 'value'
            },
            bounces: false
        });
    };
</script>
</html>